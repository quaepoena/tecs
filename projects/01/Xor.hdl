/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    And(a=a, b=b, out=outa);
    Not(in=outa, out=outb);
    Or(a=a, b=b, out=outc);
    And(a=outb, b=outc, out=out);
}